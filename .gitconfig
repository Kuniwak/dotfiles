[user]
	name = OrgaChem
	email = orga.chem.job@gmail.com
[core]
	editor = /usr/bin/vim
	excludesfile = /Users/OrgaChem/.gitignore_global
[github]
	user = OrgaChem
[credential]
	helper = osxkeychain
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = simple
[hub]
	protocol = https
